<p id="notice"><%= notice %></p>

<h1>Payment Methods</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Uuid</th>
      <th>Description</th>
      <th>Email</th>
      <th>Aap</th>
      <th>Secret</th>
      <th>Token</th>
      <th>Password</th>
      <th>User</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @payment_methods.each do |payment_method| %>
      <tr>
        <td><%= payment_method.name %></td>
        <td><%= payment_method.uuid %></td>
        <td><%= payment_method.description %></td>
        <td><%= payment_method.email %></td>
        <td><%= payment_method.aap_id %></td>
        <td><%= payment_method.secret_id %></td>
        <td><%= payment_method.token %></td>
        <td><%= payment_method.password %></td>
        <td><%= payment_method.user_id %></td>
        <td><%= payment_method.active %></td>
        <td><%= link_to 'Show', payment_method %></td>
        <td><%= link_to 'Edit', edit_payment_method_path(payment_method) %></td>
        <td><%= link_to 'Destroy', payment_method, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Payment Method', new_payment_method_path %>
