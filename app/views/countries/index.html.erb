<p id="notice"><%= notice %></p>

<h1>Countries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th>Description</th>
      <th>Cioc</th>
      <th>Currency code</th>
      <th>Currency name</th>
      <th>Currency symbol</th>
      <th>Language iso639</th>
      <th>Language name</th>
      <th>Language native name</th>
      <th>Image</th>
      <th>Date utc</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @countries.each do |country| %>
      <tr>
        <td><%= country.name %></td>
        <td><%= country.code %></td>
        <td><%= country.description %></td>
        <td><%= country.cioc %></td>
        <td><%= country.currency_code %></td>
        <td><%= country.currency_name %></td>
        <td><%= country.currency_symbol %></td>
        <td><%= country.language_iso639 %></td>
        <td><%= country.language_name %></td>
        <td><%= country.language_native_name %></td>
        <td><%= country.image %></td>
        <td><%= country.date_utc %></td>
        <td><%= country.active %></td>
        <td><%= link_to 'Show', country %></td>
        <td><%= link_to 'Edit', edit_country_path(country) %></td>
        <td><%= link_to 'Destroy', country, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Country', new_country_path %>
