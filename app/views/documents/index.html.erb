<p id="notice"><%= notice %></p>

<h1>Documents</h1>

<table>
  <thead>
    <tr>
      <th>Document</th>
      <th>Document type</th>
      <th>File</th>
      <th>Statu</th>
      <th>User</th>
      <th>Expire date</th>
      <th>Approved</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documents.each do |document| %>
      <tr>
        <td><%= document.document_id %></td>
        <td><%= document.document_type_id %></td>
        <td><%= document.file %></td>
        <td><%= document.statu_id %></td>
        <td><%= document.user_id %></td>
        <td><%= document.expire_date %></td>
        <td><%= document.approved %></td>
        <td><%= document.active %></td>
        <td><%= link_to 'Show', document %></td>
        <td><%= link_to 'Edit', edit_document_path(document) %></td>
        <td><%= link_to 'Destroy', document, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Document', new_document_path %>
