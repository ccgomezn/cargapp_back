<p id="notice"><%= notice %></p>

<h1>Cargapp Integrations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Provider</th>
      <th>Code</th>
      <th>Url</th>
      <th>Url provider</th>
      <th>Url production</th>
      <th>Url develop</th>
      <th>Email</th>
      <th>Username</th>
      <th>Password</th>
      <th>Phone</th>
      <th>Pin</th>
      <th>Token</th>
      <th>App</th>
      <th>Client</th>
      <th>Api key</th>
      <th>User</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cargapp_integrations.each do |cargapp_integration| %>
      <tr>
        <td><%= cargapp_integration.name %></td>
        <td><%= cargapp_integration.description %></td>
        <td><%= cargapp_integration.provider %></td>
        <td><%= cargapp_integration.code %></td>
        <td><%= cargapp_integration.url %></td>
        <td><%= cargapp_integration.url_provider %></td>
        <td><%= cargapp_integration.url_production %></td>
        <td><%= cargapp_integration.url_develop %></td>
        <td><%= cargapp_integration.email %></td>
        <td><%= cargapp_integration.username %></td>
        <td><%= cargapp_integration.password %></td>
        <td><%= cargapp_integration.phone %></td>
        <td><%= cargapp_integration.pin %></td>
        <td><%= cargapp_integration.token %></td>
        <td><%= cargapp_integration.app_id %></td>
        <td><%= cargapp_integration.client_id %></td>
        <td><%= cargapp_integration.api_key %></td>
        <td><%= cargapp_integration.user_id %></td>
        <td><%= cargapp_integration.active %></td>
        <td><%= link_to 'Show', cargapp_integration %></td>
        <td><%= link_to 'Edit', edit_cargapp_integration_path(cargapp_integration) %></td>
        <td><%= link_to 'Destroy', cargapp_integration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cargapp Integration', new_cargapp_integration_path %>
