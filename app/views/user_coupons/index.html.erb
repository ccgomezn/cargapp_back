<p id="notice"><%= notice %></p>

<h1>User Coupons</h1>

<table>
  <thead>
    <tr>
      <th>Discount</th>
      <th>Cargapp model</th>
      <th>Applied item</th>
      <th>Coupon</th>
      <th>User</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_coupons.each do |user_coupon| %>
      <tr>
        <td><%= user_coupon.discount %></td>
        <td><%= user_coupon.cargapp_model_id %></td>
        <td><%= user_coupon.applied_item_id %></td>
        <td><%= user_coupon.coupon_id %></td>
        <td><%= user_coupon.user_id %></td>
        <td><%= user_coupon.active %></td>
        <td><%= link_to 'Show', user_coupon %></td>
        <td><%= link_to 'Edit', edit_user_coupon_path(user_coupon) %></td>
        <td><%= link_to 'Destroy', user_coupon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Coupon', new_user_coupon_path %>
