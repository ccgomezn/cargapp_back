<p id="notice"><%= notice %></p>

<h1>User Payment Methods</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Uuid</th>
      <th>Token</th>
      <th>Card number</th>
      <th>Expiration</th>
      <th>Cvv</th>
      <th>Observation</th>
      <th>User</th>
      <th>Payment method</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_payment_methods.each do |user_payment_method| %>
      <tr>
        <td><%= user_payment_method.email %></td>
        <td><%= user_payment_method.uuid %></td>
        <td><%= user_payment_method.token %></td>
        <td><%= user_payment_method.card_number %></td>
        <td><%= user_payment_method.expiration %></td>
        <td><%= user_payment_method.cvv %></td>
        <td><%= user_payment_method.observation %></td>
        <td><%= user_payment_method.user_id %></td>
        <td><%= user_payment_method.payment_method_id %></td>
        <td><%= user_payment_method.active %></td>
        <td><%= link_to 'Show', user_payment_method %></td>
        <td><%= link_to 'Edit', edit_user_payment_method_path(user_payment_method) %></td>
        <td><%= link_to 'Destroy', user_payment_method, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Payment Method', new_user_payment_method_path %>
