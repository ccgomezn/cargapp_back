<p id="notice"><%= notice %></p>

<h1>Vehicle Documents</h1>

<table>
  <thead>
    <tr>
      <th>Document type</th>
      <th>File</th>
      <th>Statu</th>
      <th>User</th>
      <th>Vehicle</th>
      <th>Expire date</th>
      <th>Approved</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicle_documents.each do |vehicle_document| %>
      <tr>
        <td><%= vehicle_document.document_type_id %></td>
        <td><%= vehicle_document.file %></td>
        <td><%= vehicle_document.statu_id %></td>
        <td><%= vehicle_document.user_id %></td>
        <td><%= vehicle_document.vehicle_id %></td>
        <td><%= vehicle_document.expire_date %></td>
        <td><%= vehicle_document.approved %></td>
        <td><%= vehicle_document.active %></td>
        <td><%= link_to 'Show', vehicle_document %></td>
        <td><%= link_to 'Edit', edit_vehicle_document_path(vehicle_document) %></td>
        <td><%= link_to 'Destroy', vehicle_document, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehicle Document', new_vehicle_document_path %>
